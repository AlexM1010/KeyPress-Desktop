{
  "name": "@keypress/schemas",
  "version": "1.0.0",
  "description": "Shared schema definitions for Keypress automation tool",
  "type": "module",
  "scripts": {
    "build": "npm run generate-go-structs",
    "compile": "tsc",
    "generate-json-schema": "node scripts/generate-json-schema.js",
    "generate-go-structs": "quicktype --src-lang json --lang go --package schema --out ../../internal/schema/generated.go ./sample-data.json",
    "validate": "tsc --noEmit"
  },
  "devDependencies": {
    "quicktype": "^23.2.6",
    "typescript": "^5.6.3",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.22.4"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}